{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Footer from \"../components/Footer.vue\";\nexport default {\n  name: \"UserAddress\",\n  data() {\n    return {\n      businessId: this.$route.query.businessId,\n      user: {},\n      deliveryAddressArr: []\n    };\n  },\n  created() {\n    this.user = this.$getSessionStorage(\"user\");\n    this.listDeliveryAddressByUserId();\n  },\n  components: {\n    Footer\n  },\n  methods: {\n    sexFilter(userSex) {\n      return userSex == 1 ? \"先生\" : \"女士\";\n    },\n    listDeliveryAddressByUserId() {\n      //查询送货地址\n      let url = \"DeliveryAddressController/listDeliveryAddressByUserId/\" + this.user.userId;\n      this.$axios.get(url).then(response => {\n        this.deliveryAddressArr = response.data.result;\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    setDeliveryAddress(deliveryAddress) {\n      //把用户选择的默认送货地址存储到localStorage中\n      this.$setLocalStorage(this.user.userId, deliveryAddress);\n      this.$router.push({\n        path: \"/orders\",\n        query: {\n          businessId: this.businessId\n        }\n      });\n    },\n    toAddUserAddress() {\n      this.$router.push({\n        path: \"/addUserAddress\",\n        query: {\n          businessId: this.businessId\n        }\n      });\n    },\n    editUserAddress(daId) {\n      this.$router.push({\n        path: \"/editUserAddress\",\n        query: {\n          businessId: this.businessId,\n          daId: daId\n        }\n      });\n    },\n    removeUserAddress(daId) {\n      if (!confirm(\"确认要删除此送货地址吗？\")) {\n        return;\n      }\n      let url = \"DeliveryAddressController/removeDeliveryAddress/\" + daId;\n      this.$axios.delete(url).then(response => {\n        if (response.data.result > 0) {\n          let deliveryAddress = this.$getLocalStorage(this.user.userId);\n          if (deliveryAddress != null && deliveryAddress.daId == daId) {\n            this.$removeLocalStorage(this.user.userId);\n          }\n          this.listDeliveryAddressByUserId();\n        } else {\n          alert(\"删除地址失败！\");\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["Footer","name","data","businessId","$route","query","user","deliveryAddressArr","created","$getSessionStorage","listDeliveryAddressByUserId","components","methods","sexFilter","userSex","url","userId","$axios","get","then","response","result","catch","error","console","setDeliveryAddress","deliveryAddress","$setLocalStorage","$router","push","path","toAddUserAddress","editUserAddress","daId","removeUserAddress","confirm","delete","$getLocalStorage","$removeLocalStorage","alert"],"sources":["C:\\WAILE\\Homework\\elm\\vue_springcloud\\src\\views\\UserAddress.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <!-- header部分 -->\r\n    <header>\r\n      <p>地址管理</p>\r\n    </header>\r\n    <!-- 地址列表部分 -->\r\n    <ul class=\"addresslist\">\r\n      <li v-for=\"item in deliveryAddressArr\">\r\n        <div class=\"addresslist-left\" @click=\"setDeliveryAddress(item)\">\r\n          <h3>\r\n            {{ item.contactName }}{{ sexFilter(item.contactSex) }}\r\n            {{ item.contactTel }}\r\n          </h3>\r\n          <p>{{ item.address }}</p>\r\n        </div>\r\n        <div class=\"addresslist-right\">\r\n          <i class=\"fa fa-edit\" @click=\"editUserAddress(item.daId)\"></i>\r\n          <i class=\"fa fa-remove\" @click=\"removeUserAddress(item.daId)\"></i>\r\n        </div>\r\n      </li>\r\n    </ul>\r\n    <!-- 新增地址部分 -->\r\n    <div class=\"addbtn\" @click=\"toAddUserAddress\">\r\n      <button>\r\n        <i class=\"fa fa-plus-circle\"></i>\r\n        新增收货地址\r\n      </button>\r\n    </div>\r\n    <!-- 底部菜单部分 -->\r\n    <Footer></Footer>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Footer from \"../components/Footer.vue\";\r\nexport default {\r\n  name: \"UserAddress\",\r\n  data() {\r\n    return {\r\n      businessId: this.$route.query.businessId,\r\n      user: {},\r\n      deliveryAddressArr: [],\r\n    };\r\n  },\r\n  created() {\r\n    this.user = this.$getSessionStorage(\"user\");\r\n\r\n    this.listDeliveryAddressByUserId();\r\n  },\r\n  components: {\r\n    Footer,\r\n  },\r\n  methods: {\r\n    sexFilter(userSex) {\r\n      return userSex == 1 ? \"先生\" : \"女士\";\r\n    },\r\n    listDeliveryAddressByUserId() {\r\n      //查询送货地址\r\n      let url =\r\n        \"DeliveryAddressController/listDeliveryAddressByUserId/\" +\r\n        this.user.userId;\r\n      this.$axios\r\n        .get(url)\r\n        .then((response) => {\r\n          this.deliveryAddressArr = response.data.result;\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n    setDeliveryAddress(deliveryAddress) {\r\n      //把用户选择的默认送货地址存储到localStorage中\r\n      this.$setLocalStorage(this.user.userId, deliveryAddress);\r\n      this.$router.push({\r\n        path: \"/orders\",\r\n        query: {\r\n          businessId: this.businessId,\r\n        },\r\n      });\r\n    },\r\n    toAddUserAddress() {\r\n      this.$router.push({\r\n        path: \"/addUserAddress\",\r\n        query: {\r\n          businessId: this.businessId,\r\n        },\r\n      });\r\n    },\r\n    editUserAddress(daId) {\r\n      this.$router.push({\r\n        path: \"/editUserAddress\",\r\n        query: {\r\n          businessId: this.businessId,\r\n          daId: daId,\r\n        },\r\n      });\r\n    },\r\n    removeUserAddress(daId) {\r\n      if (!confirm(\"确认要删除此送货地址吗？\")) {\r\n        return;\r\n      }\r\n\r\n      let url = \"DeliveryAddressController/removeDeliveryAddress/\" + daId;\r\n      this.$axios\r\n        .delete(url)\r\n        .then((response) => {\r\n          if (response.data.result > 0) {\r\n            let deliveryAddress = this.$getLocalStorage(this.user.userId);\r\n            if (deliveryAddress != null && deliveryAddress.daId == daId) {\r\n              this.$removeLocalStorage(this.user.userId);\r\n            }\r\n            this.listDeliveryAddressByUserId();\r\n          } else {\r\n            alert(\"删除地址失败！\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n/*************** 总容器 ***************/\r\n.wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: #f2f2f2;\r\n}\r\n\r\n/*************** header ***************/\r\n.wrapper header {\r\n  width: 100%;\r\n  height: 12vw;\r\n  background-color: deepskyblue;\r\n  display: flex;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  color: white;\r\n  font-size: 5vw;\r\n  position: fixed;\r\n  left: 0;\r\n  top: 0;\r\n  /*保证在最上层*/\r\n  z-index: 1000;\r\n}\r\n\r\n/*************** addresslist ***************/\r\n.wrapper .addresslist {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding-top: 15vw;\r\n}\r\n\r\n.wrapper .addresslist li {\r\n  width: 95%;\r\n  padding: 5vw 3vw;\r\n  box-sizing: border-box;\r\n  margin: 0 2.5vw 3vw;\r\n\r\n  background-color: white;\r\n  border-radius: 5vw;\r\n  border: solid 1.5px #ddd;\r\n\r\n  display: flex;\r\n  justify-content: space-between;\r\n}\r\n\r\n.wrapper .addresslist li .addresslist-left {\r\n  flex: 5;\r\n  /*左边这块区域是可以点击的*/\r\n  user-select: none;\r\n  cursor: pointer;\r\n}\r\n\r\n.wrapper .addresslist li .addresslist-left h3 {\r\n  color: darkslategray;\r\n  font-size: 3.5vw;\r\n  font-weight: 500;\r\n  margin-bottom: 1vw;\r\n}\r\n\r\n.wrapper .addresslist li .addresslist-left p {\r\n  font-size: 3vw;\r\n  color: darkslategray;\r\n}\r\n\r\n.wrapper .addresslist li .addresslist-right {\r\n  font-size: 5vw;\r\n  color: gray;\r\n  display: flex;\r\n  align-items: center;\r\n  user-select: none;\r\n}\r\n.wrapper .addresslist li .addresslist-right .fa-remove {\r\n  margin-left: 3vw;\r\n  cursor: pointer;\r\n}\r\n.wrapper .addresslist li .addresslist-right .fa-edit {\r\n  cursor: pointer;\r\n}\r\n\r\n/*************** 新增地址部分 ***************/\r\n.wrapper .addbtn {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding: 4vw 10vw 16vw;\r\n  display: flex;\r\n  justify-content: center;\r\n}\r\n.wrapper .addbtn button {\r\n  width: 100%;\r\n  height: 10vw;\r\n  background-color: deepskyblue;\r\n  border: none;\r\n  outline: none;\r\n  border-radius: 4vw;\r\n\r\n  font-size: 3.5vw;\r\n  color: white;\r\n\r\n  cursor: pointer;\r\n}\r\n.wrapper .addbtn button .fa-plus-circle {\r\n  margin-right: 2vw;\r\n}\r\n</style>\r\n"],"mappings":";AAkCA,OAAOA,MAAK,MAAO,0BAA0B;AAC7C,eAAe;EACbC,IAAI,EAAE,aAAa;EACnBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,UAAU;MACxCG,IAAI,EAAE,CAAC,CAAC;MACRC,kBAAkB,EAAE;IACtB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACF,IAAG,GAAI,IAAI,CAACG,kBAAkB,CAAC,MAAM,CAAC;IAE3C,IAAI,CAACC,2BAA2B,CAAC,CAAC;EACpC,CAAC;EACDC,UAAU,EAAE;IACVX;EACF,CAAC;EACDY,OAAO,EAAE;IACPC,SAASA,CAACC,OAAO,EAAE;MACjB,OAAOA,OAAM,IAAK,IAAI,IAAG,GAAI,IAAI;IACnC,CAAC;IACDJ,2BAA2BA,CAAA,EAAG;MAC5B;MACA,IAAIK,GAAE,GACJ,wDAAuD,GACvD,IAAI,CAACT,IAAI,CAACU,MAAM;MAClB,IAAI,CAACC,MAAK,CACPC,GAAG,CAACH,GAAG,EACPI,IAAI,CAAEC,QAAQ,IAAK;QAClB,IAAI,CAACb,kBAAiB,GAAIa,QAAQ,CAAClB,IAAI,CAACmB,MAAM;MAChD,CAAC,EACAC,KAAK,CAAEC,KAAK,IAAK;QAChBC,OAAO,CAACD,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN,CAAC;IACDE,kBAAkBA,CAACC,eAAe,EAAE;MAClC;MACA,IAAI,CAACC,gBAAgB,CAAC,IAAI,CAACrB,IAAI,CAACU,MAAM,EAAEU,eAAe,CAAC;MACxD,IAAI,CAACE,OAAO,CAACC,IAAI,CAAC;QAChBC,IAAI,EAAE,SAAS;QACfzB,KAAK,EAAE;UACLF,UAAU,EAAE,IAAI,CAACA;QACnB;MACF,CAAC,CAAC;IACJ,CAAC;IACD4B,gBAAgBA,CAAA,EAAG;MACjB,IAAI,CAACH,OAAO,CAACC,IAAI,CAAC;QAChBC,IAAI,EAAE,iBAAiB;QACvBzB,KAAK,EAAE;UACLF,UAAU,EAAE,IAAI,CAACA;QACnB;MACF,CAAC,CAAC;IACJ,CAAC;IACD6B,eAAeA,CAACC,IAAI,EAAE;MACpB,IAAI,CAACL,OAAO,CAACC,IAAI,CAAC;QAChBC,IAAI,EAAE,kBAAkB;QACxBzB,KAAK,EAAE;UACLF,UAAU,EAAE,IAAI,CAACA,UAAU;UAC3B8B,IAAI,EAAEA;QACR;MACF,CAAC,CAAC;IACJ,CAAC;IACDC,iBA