{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nexport default {\n  name: 'Register',\n  data() {\n    return {\n      user: {\n        userId: '',\n        password: '',\n        userName: '',\n        userSex: 1\n      },\n      confirmPassword: ''\n    };\n  },\n  methods: {\n    checkUserId() {\n      let url = \"http://localhost:10100/UserController/getUserById/${this.userId}\";\n      this.$axios.get(url).then(response => {\n        if (response.data.result == 1) {\n          this.user.userId = '';\n          alert('此手机号码已存在！');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    },\n    register() {\n      if (this.user.userId == '') {\n        alert('手机号码不能为空！');\n        return;\n      }\n      if (this.user.password == '') {\n        alert('密码不能为空！');\n        return;\n      }\n      if (this.user.password != this.confirmPassword) {\n        alert('两次输入的密码不一致！');\n        return;\n      }\n      if (this.user.userName == '') {\n        alert('用户名不能为空！');\n        return;\n      }\n      //注册请求\n      let url = \"http://localhost:10100/UserController/saveUser/${this.user.userId}/${this.user.password}/${this.user.userName}/${this.user.userSex}\";\n      this.$axios.post(url).then(response => {\n        // console.log(response.data);\n        if (response.data.result > 0) {\n          alert('注册成功！');\n          this.$router.push('/login');\n        } else {\n          alert('注册失败！');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["name","data","user","userId","password","userName","userSex","confirmPassword","methods","checkUserId","url","$axios","get","then","response","result","alert","catch","error","console","register","post","$router","push"],"sources":["C:\\Users\\admin\\Desktop\\elm_vue3\\src\\views\\Register.vue"],"sourcesContent":["<template>\r\n\t<div class=\"wrapper\">\r\n\t\t<!-- header部分 -->\r\n\t\t<header>\r\n\t\t\t<p>