{"ast":null,"code":"import Footer from \"../components/Footer.vue\";\nexport default {\n  name: \"UpdateUserName\",\n  data() {\n    return {\n      newName: \"\",\n      user: {}\n    };\n  },\n  components: {\n    Footer\n  },\n  created() {\n    this.user = this.$getSessionStorage(\"user\");\n  },\n  methods: {\n    toUpdateUserMsg() {\n      //根据userId更改userName\n      if (this.newName == \"\") {\n        alert(\"用户名不能为空！\");\n        return;\n      }\n      let url = `UserController/updateUserById/${this.user.userId}/${this.userPassword}/${this.newName}/${this.user.userSex}`;\n      this.$axios.put(url).then(response => {\n        console.log(response.data.result);\n        if (response.data.result > 0) {\n          this.user.userName = this.newName;\n          this.$setSessionStorage(\"user\", this.user);\n          alert(\"修改用户名成功！\");\n        } else {\n          alert(\"修改用户名失败！\");\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["Footer","name","data","newName","user","components","created","$getSessionStorage","methods","toUpdateUserMsg","alert","url","userId","userPassword","userSex","$axios","put","then","response","console","log","result","userName","$setSessionStorage","catch","error"],"sources":["C:\\WAILE\\Homework\\elm\\vue_springcloud\\src\\views\\UpdateUserName.vue"],"sourcesContent":["<template>\r\n  <!-- 总容器 -->\r\n  <div class=\"wrapper\">\r\n    <!-- header部分 -->\r\n    <header>\r\n      <p>更新用户名</p>\r\n    </header>\r\n    <!-- 更新用户名部分 -->\r\n    <div class=\"form-box\">\r\n      <li>\r\n        <div class=\"title\">用户名:</div>\r\n        <div class=\"content\">\r\n          <input type=\"text\" v-model=\"newName\" placeholder=\"用户名\" />\r\n        </div>\r\n      </li>\r\n    </div>\r\n    <!--保存按钮-->\r\n    <div class=\"save-button\" @click=\"toUpdateUserMsg()\">\r\n      <button>保存</button>\r\n    </div>\r\n    <!-- 底部菜单部分 -->\r\n    <Footer></Footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Footer from \"../components/Footer.vue\";\r\n\r\nexport default {\r\n  name: \"UpdateUserName\",\r\n  data() {\r\n    return {\r\n      newName: \"\",\r\n      user: {},\r\n    };\r\n  },\r\n  components: {\r\n    Footer,\r\n  },\r\n  created() {\r\n    this.user = this.$getSessionStorage(\"user\");\r\n  },\r\n  methods: {\r\n    toUpdateUserMsg() {\r\n      //根据userId更改userName\r\n      if (this.newName == \"\") {\r\n        alert(\"用户名不能为空！\");\r\n        return;\r\n      }\r\n      let url = `UserController/updateUserById/${this.user.userId}/${this.userPassword}/${this.newName}/${this.user.userSex}`;\r\n      this.$axios\r\n        .put(url)\r\n        .then((response) => {\r\n          console.log(response.data.result);\r\n          if (response.data.result > 0) {\r\n            this.user.userName = this.newName;\r\n            this.$setSessionStorage(\"user\", this.user);\r\n            alert(\"修改用户名成功！\");\r\n          } else {\r\n            alert(\"修改用户名失败！\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/****************** 总容器 ******************/\r\n.wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n/****************** header ******************/\r\n.wrapper header {\r\n  width: 100%;\r\n  height: 12vw;\r\n  background-color: deepskyblue;\r\n  color: #fff;\r\n  font-size: 4.8vw;\r\n  position: fixed;\r\n  left: 0;\r\n  top: 0;\r\n  z-index: 1000;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n/****************** 修改用户名部分 ******************/\r\n.wrapper .form-box {\r\n  width: 100%;\r\n  padding-top: 12vw;\r\n}\r\n\r\n.wrapper .form-box li {\r\n  display: flex;\r\n  align-items: center;\r\n  box-sizing: border-box;\r\n  padding: 3vw 3vw 0;\r\n}\r\n\r\n.wrapper .form-box li .title {\r\n  flex: 0 0 18vw;\r\n  font-size: 3.5vw;\r\n  font-weight: 700;\r\n  color: darkslategray;\r\n}\r\n\r\n.wrapper .form-box li .content {\r\n  flex: 1;\r\n}\r\n\r\n.wrapper .form-box li .content input {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding: 4vw 2vw;\r\n  border-radius: 3vw;\r\n  background-color: white;\r\n\r\n  font-size: 3vw;\r\n  border: none;\r\n  outline: none;\r\n}\r\n\r\n/****************** 保存按钮部分 ******************/\r\n.wrapper .save-button {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding: 4vw 10vw 0;\r\n}\r\n\r\n.wrapper .save-button button {\r\n  width: 100%;\r\n  height: 10vw;\r\n  background-color: mediumseagreen;\r\n  border: none;\r\n  outline: none;\r\n  border-radius: 4vw;\r\n\r\n  font-size: 3.5vw;\r\n  color: white;\r\n\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":"AA0BA,OAAOA,MAAK,MAAO,0BAA0B;AAE7C,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,IAAI,EAAE,CAAC;IACT,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVL;EACF,CAAC;EACDM,OAAOA,CAAA,EAAG;IACR,IAAI,CAACF,IAAG,GAAI,IAAI,CAACG,kBAAkB,CAAC,MAAM,CAAC;EAC7C,CAAC;EACDC,OAAO,EAAE;IACPC,eAAeA,CAAA,EAAG;MAChB;MACA,IAAI,IAAI,CAACN,OAAM,IAAK,EAAE,EAAE;QACtBO,KAAK,CAAC,UAAU,CAAC;QACjB;MACF;MACA,IAAIC,GAAE,GAAK,iCAAgC,IAAI,CAACP,IAAI,CAACQ,MAAO,IAAG,IAAI,CAACC,YAAa,IAAG,IAAI,CAACV,OAAQ,IAAG,IAAI,CAACC,IAAI,CAACU,OAAQ,EAAC;MACvH,IAAI,CAACC,MAAK,CACPC,GAAG,CAACL,GAAG,EACPM,IAAI,CAAEC,QAAQ,IAAK;QAClBC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAChB,IAAI,CAACmB,MAAM,CAAC;QACjC,IAAIH,QAAQ,CAAChB,IAAI,CAACmB,MAAK,GAAI,CAAC,EAAE;UAC5B,IAAI,CAACjB,IAAI,CAACkB,QAAO,GAAI,IAAI,CAACnB,OAAO;UACjC,IAAI,CAACoB,kBAAkB,CAAC,MAAM,EAAE,IAAI,CAACnB,IAAI,CAAC;UAC1CM,KAAK,CAAC,UAAU,CAAC;QACnB,OAAO;UACLA,KAAK,CAAC,UAAU,CAAC;QACnB;MACF,CAAC,EACAc,KAAK,CAAEC,KAAK,IAAK;QAChBN,OAAO,CAACM,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN;EACF;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}