{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Footer from \"../components/Footer.vue\";\nexport default {\n  name: \"EditUserAddress\",\n  data() {\n    return {\n      businessId: this.$route.query.businessId,\n      daId: this.$route.query.daId,\n      user: {},\n      deliveryAddress: {}\n    };\n  },\n  created() {\n    this.user = this.$getSessionStorage(\"user\");\n    let url = \"DeliveryAddressController/getDeliveryAddressById/\" + this.daId;\n    this.$axios.get(url).then(response => {\n      this.deliveryAddress = response.data.result;\n    }).catch(error => {\n      console.error(error);\n    });\n  },\n  components: {\n    Footer\n  },\n  methods: {\n    editUserAddress() {\n      if (this.deliveryAddress.contactName == \"\") {\n        alert(\"联系人姓名不能为空！\");\n        return;\n      }\n      if (this.deliveryAddress.contactTel == \"\") {\n        alert(\"联系人电话不能为空！\");\n        return;\n      }\n      if (this.deliveryAddress.address == \"\") {\n        alert(\"联系人地址不能为空！\");\n        return;\n      }\n      let url = \"DeliveryAddressController/updateDeliveryAddress/\" + this.daId + \"/\" + this.deliveryAddress.contactName + \"/\" + this.deliveryAddress.contactSex + \"/\" + this.deliveryAddress.contactTel + \"/\" + this.deliveryAddress.address;\n      this.$axios.put(url).then(response => {\n        if (response.data.result > 0) {\n          this.$router.push({\n            path: \"/userAddress\",\n            query: {\n              businessId: this.businessId\n            }\n          });\n        } else {\n          alert(\"更新地址失败！\");\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["Footer","name","data","businessId","$route","query","daId","user","deliveryAddress","created","$getSessionStorage","url","$axios","get","then","response","result","catch","error","console","components","methods","editUserAddress","contactName","alert","contactTel","address","contactSex","put","$router","push","path"],"sources":["C:\\WAILE\\Homework\\elm\\vue_springcloud\\src\\views\\EditUserAddress.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <!-- header部分 -->\r\n    <header>\r\n      <p>编辑送货地址</p>\r\n    </header>\r\n    <!-- 表单部分 -->\r\n    <ul class=\"form-box\">\r\n      <li>\r\n        <div class=\"title\">联系人：</div>\r\n        <div class=\"content\">\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"deliveryAddress.contactName\"\r\n            placeholder=\"联系人\r\n姓名\"\r\n          />\r\n        </div>\r\n      </li>\r\n      <li>\r\n        <div class=\"title\">性别：</div>\r\n        <div class=\"content\" style=\"font-size: 3vw\">\r\n          <input\r\n            type=\"radio\"\r\n            v-model=\"deliveryAddress.contactSex\"\r\n            value=\"1\"\r\n            style=\"width: 6vw; height: 3.2vw\"\r\n            checked\r\n          />男\r\n          <input\r\n            type=\"radio\"\r\n            v-model=\"deliveryAddress.contactSex\"\r\n            value=\"0\"\r\n            style=\"width: 6vw; height: 3.2vw\"\r\n          />女\r\n        </div>\r\n      </li>\r\n      <li>\r\n        <div class=\"title\">电话：</div>\r\n        <div class=\"content\">\r\n          <input\r\n            type=\"tel\"\r\n            v-model=\"deliveryAddress.contactTel\"\r\n            placeholder=\"电话\"\r\n          />\r\n        </div>\r\n      </li>\r\n      <li>\r\n        <div class=\"title\">收货地址：</div>\r\n        <div class=\"content\">\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"deliveryAddress.address\"\r\n            placeholder=\"收货地址\"\r\n          />\r\n        </div>\r\n      </li>\r\n    </ul>\r\n    <div class=\"button-add\">\r\n      <button @click=\"editUserAddress\">更新</button>\r\n    </div>\r\n    <!-- 底部菜单部分 -->\r\n    <Footer></Footer>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Footer from \"../components/Footer.vue\";\r\nexport default {\r\n  name: \"EditUserAddress\",\r\n  data() {\r\n    return {\r\n      businessId: this.$route.query.businessId,\r\n      daId: this.$route.query.daId,\r\n      user: {},\r\n      deliveryAddress: {},\r\n    };\r\n  },\r\n  created() {\r\n    this.user = this.$getSessionStorage(\"user\");\r\n    let url = \"DeliveryAddressController/getDeliveryAddressById/\" + this.daId;\r\n    this.$axios\r\n      .get(url)\r\n      .then((response) => {\r\n        this.deliveryAddress = response.data.result;\r\n      })\r\n      .catch((error) => {\r\n        console.error(error);\r\n      });\r\n  },\r\n  components: {\r\n    Footer,\r\n  },\r\n  methods: {\r\n    editUserAddress() {\r\n      if (this.deliveryAddress.contactName == \"\") {\r\n        alert(\"联系人姓名不能为空！\");\r\n        return;\r\n      }\r\n      if (this.deliveryAddress.contactTel == \"\") {\r\n        alert(\"联系人电话不能为空！\");\r\n        return;\r\n      }\r\n      if (this.deliveryAddress.address == \"\") {\r\n        alert(\"联系人地址不能为空！\");\r\n        return;\r\n      }\r\n      let url =\r\n        \"DeliveryAddressController/updateDeliveryAddress/\" +\r\n        this.daId +\r\n        \"/\" +\r\n        this.deliveryAddress.contactName +\r\n        \"/\" +\r\n        this.deliveryAddress.contactSex +\r\n        \"/\" +\r\n        this.deliveryAddress.contactTel +\r\n        \"/\" +\r\n        this.deliveryAddress.address;\r\n      this.$axios\r\n        .put(url)\r\n        .then((response) => {\r\n          if (response.data.result > 0) {\r\n            this.$router.push({\r\n              path: \"/userAddress\",\r\n              query: {\r\n                businessId: this.businessId,\r\n              },\r\n            });\r\n          } else {\r\n            alert(\"更新地址失败！\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n/*************** 总容器 ***************/\r\n.wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: #f2f2f2;\r\n}\r\n\r\n/*************** header ***************/\r\n.wrapper header {\r\n  width: 100%;\r\n  height: 12vw;\r\n  background-color: deepskyblue;\r\n\r\n  position: fixed;\r\n  top: 0;\r\n  left: 0;\r\n\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n\r\n  color: white;\r\n  font-size: 5vw;\r\n  font-weight: 700;\r\n}\r\n\r\n/*************** （表单信息） ***************/\r\n.wrapper .form-box {\r\n  width: 100%;\r\n  margin-top: 12vw;\r\n}\r\n\r\n.wrapper .form-box li {\r\n  display: flex;\r\n  align-items: center;\r\n  box-sizing: border-box;\r\n  padding: 3vw 3vw 0;\r\n}\r\n\r\n.wrapper .form-box li .title {\r\n  flex: 0 0 18vw;\r\n  font-size: 3.5vw;\r\n  font-weight: 700;\r\n  color: darkslategray;\r\n}\r\n\r\n.wrapper .form-box li .content {\r\n  flex: 1;\r\n}\r\n\r\n.wrapper .form-box li .content input {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding: 4vw 2vw;\r\n  border-radius: 3vw;\r\n\r\n  font-size: 3vw;\r\n  border: none;\r\n  outline: none;\r\n}\r\n\r\n.wrapper .button-add {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding: 4vw 10vw 20vw;\r\n}\r\n\r\n.wrapper .button-add button {\r\n  width: 100%;\r\n  height: 10vw;\r\n  font-size: 3.5vw;\r\n  color: white;\r\n  background-color: mediumseagreen;\r\n  border: none;\r\n  outline: none;\r\n  border-radius: 4vw;\r\n\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":";AAkEA,OAAOA,MAAK,MAAO,0BAA0B;AAC7C,eAAe;EACbC,IAAI,EAAE,iBAAiB;EACvBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,UAAU;MACxCG,IAAI,EAAE,IAAI,CAACF,MAAM,CAACC,KAAK,CAACC,IAAI;MAC5BC,IAAI,EAAE,CAAC,CAAC;MACRC,eAAe,EAAE,CAAC;IACpB,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACF,IAAG,GAAI,IAAI,CAACG,kBAAkB,CAAC,MAAM,CAAC;IAC3C,IAAIC,GAAE,GAAI,mDAAkD,GAAI,IAAI,CAACL,IAAI;IACzE,IAAI,CAACM,MAAK,CACPC,GAAG,CAACF,GAAG,EACPG,IAAI,CAAEC,QAAQ,IAAK;MAClB,IAAI,CAACP,eAAc,GAAIO,QAAQ,CAACb,IAAI,CAACc,