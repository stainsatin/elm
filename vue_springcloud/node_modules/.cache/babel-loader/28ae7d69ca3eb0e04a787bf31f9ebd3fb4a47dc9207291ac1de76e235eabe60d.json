{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Footer from \"../components/Footer.vue\";\nexport default {\n  name: \"AddUserAddress\",\n  data() {\n    return {\n      businessId: this.$route.query.businessId,\n      user: {},\n      deliveryAddress: {\n        contactName: \"\",\n        contactSex: 1,\n        contactTel: \"\",\n        address: \"\"\n      }\n    };\n  },\n  created() {\n    this.user = this.$getSessionStorage(\"user\");\n  },\n  components: {\n    Footer\n  },\n  methods: {\n    addUserAddress() {\n      if (this.deliveryAddress.contactName == \"\") {\n        alert(\"联系人姓名不能为空！\");\n        return;\n      }\n      if (this.deliveryAddress.contactTel == \"\") {\n        alert(\"联系人电话不能为空！\");\n        return;\n      }\n      if (this.deliveryAddress.address == \"\") {\n        alert(\"联系人地址不能为空！\");\n        return;\n      }\n      console.log(this.deliveryAddress);\n      let url = \"DeliveryAddressController/saveDeliveryAddress/\" + this.deliveryAddress.contactName + \"/\" + this.deliveryAddress.contactSex + \"/\" + this.deliveryAddress.contactTel + \"/\" + this.deliveryAddress.address + \"/\" + this.user.userId;\n      this.$axios.post(url).then(response => {\n        if (response.data.result > 0) {\n          this.$router.push({\n            path: \"/userAddress\",\n            query: {\n              businessId: this.businessId\n            }\n          });\n        } else {\n          alert(\"新增地址失败！\");\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["Footer","name","data","businessId","$route","query","user","deliveryAddress","contactName","contactSex","contactTel","address","created","$getSessionStorage","components","methods","addUserAddress","alert","console","log","url","userId","$axios","post","then","response","result","$router","push","path","catch","error"],"sources":["C:\\WAILE\\Homework\\elm\\vue_springcloud\\src\\views\\AddUserAddress.vue"],"sourcesContent":["<template>\r\n  <div class=\"wrapper\">\r\n    <!-- header部分 -->\r\n    <header>\r\n      <p>新增送货地址</p>\r\n    </header>\r\n    <!-- 表单部分 -->\r\n    <ul class=\"form-box\">\r\n      <li>\r\n        <div class=\"title\">联系人：</div>\r\n        <div class=\"content\">\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"deliveryAddress.contactName\"\r\n            placeholder=\"联系人\r\n姓名\"\r\n          />\r\n        </div>\r\n      </li>\r\n      <li>\r\n        <div class=\"title\">性别：</div>\r\n        <div class=\"content\" style=\"font-size: 3vw\">\r\n          <input\r\n            type=\"radio\"\r\n            v-model=\"deliveryAddress.contactSex\"\r\n            value=\"1\"\r\n            style=\"width: 6vw; height: 3.2vw\"\r\n          />男\r\n          <input\r\n            type=\"radio\"\r\n            v-model=\"deliveryAddress.contactSex\"\r\n            value=\"0\"\r\n            style=\"width: 6vw; height: 3.2vw\"\r\n          />女\r\n        </div>\r\n      </li>\r\n      <li>\r\n        <div class=\"title\">电话：</div>\r\n        <div class=\"content\">\r\n          <input\r\n            type=\"tel\"\r\n            v-model=\"deliveryAddress.contactTel\"\r\n            placeholder=\"电话\"\r\n          />\r\n        </div>\r\n      </li>\r\n      <li>\r\n        <div class=\"title\">收货地址：</div>\r\n        <div class=\"content\">\r\n          <input\r\n            type=\"text\"\r\n            v-model=\"deliveryAddress.address\"\r\n            placeholder=\"收货地址\"\r\n          />\r\n        </div>\r\n      </li>\r\n    </ul>\r\n    <div class=\"button-add\">\r\n      <button @click=\"addUserAddress\">保存</button>\r\n    </div>\r\n    <!-- 底部菜单部分 -->\r\n    <Footer></Footer>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Footer from \"../components/Footer.vue\";\r\nexport default {\r\n  name: \"AddUserAddress\",\r\n  data() {\r\n    return {\r\n      businessId: this.$route.query.businessId,\r\n      user: {},\r\n      deliveryAddress: {\r\n        contactName: \"\",\r\n        contactSex: 1,\r\n        contactTel: \"\",\r\n        address: \"\",\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.user = this.$getSessionStorage(\"user\");\r\n  },\r\n  components: {\r\n    Footer,\r\n  },\r\n  methods: {\r\n    addUserAddress() {\r\n      if (this.deliveryAddress.contactName == \"\") {\r\n        alert(\"联系人姓名不能为空！\");\r\n        return;\r\n      }\r\n      if (this.deliveryAddress.contactTel == \"\") {\r\n        alert(\"联系人电话不能为空！\");\r\n        return;\r\n      }\r\n      if (this.deliveryAddress.address == \"\") {\r\n        alert(\"联系人地址不能为空！\");\r\n        return;\r\n      }\r\n\r\n      console.log(this.deliveryAddress);\r\n      let url =\r\n        \"DeliveryAddressController/saveDeliveryAddress/\" +\r\n        this.deliveryAddress.contactName +\r\n        \"/\" +\r\n        this.deliveryAddress.contactSex +\r\n        \"/\" +\r\n        this.deliveryAddress.contactTel +\r\n        \"/\" +\r\n        this.deliveryAddress.address +\r\n        \"/\" +\r\n        this.user.userId;\r\n      this.$axios\r\n        .post(url)\r\n        .then((response) => {\r\n          if (response.data.result > 0) {\r\n            this.$router.push({\r\n              path: \"/userAddress\",\r\n              query: {\r\n                businessId: this.businessId,\r\n              },\r\n            });\r\n          } else {\r\n            alert(\"新增地址失败！\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n/*************** 总容器 ***************/\r\n.wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n  background-color: #f2f2f2;\r\n}\r\n\r\n/*************** header ***************/\r\n.wrapper header {\r\n  width: 100%;\r\n  height: 12vw;\r\n  background-color: deepskyblue;\r\n  display: flex;\r\n  justify-content: space-around;\r\n  align-items: center;\r\n  color: white;\r\n  font-size: 5vw;\r\n  position: fixed;\r\n  left: 0;\r\n  top: 0;\r\n  /*保证在最上层*/\r\n  z-index: 1000;\r\n}\r\n\r\n/*************** （表单信息） ***************/\r\n.wrapper .form-box {\r\n  width: 100%;\r\n  margin-top: 12vw;\r\n}\r\n\r\n.wrapper .form-box li {\r\n  display: flex;\r\n  align-items: center;\r\n  box-sizing: border-box;\r\n  padding: 3vw 3vw 0;\r\n}\r\n\r\n.wrapper .form-box li .title {\r\n  flex: 0 0 18vw;\r\n  font-size: 3.5vw;\r\n  font-weight: 700;\r\n  color: darkslategray;\r\n}\r\n\r\n.wrapper .form-box li .content {\r\n  flex: 1;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.wrapper .form-box li .content input {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding: 4vw 2vw;\r\n  border-radius: 1vw;\r\n\r\n  font-size: 3vw;\r\n  border: none;\r\n  outline: none;\r\n}\r\n\r\n.wrapper .button-add {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding: 4vw 10vw 20vw;\r\n}\r\n\r\n.wrapper .button-add button {\r\n  width: 100%;\r\n  height: 10vw;\r\n  font-size: 3.5vw;\r\n  font-weight: 700;\r\n  border: none;\r\n  outline: none;\r\n  border-radius: 4vw;\r\n  color: white;\r\n  background-color: mediumseagreen;\r\n}\r\n</style>\r\n"],"mappings":";AAiEA,OAAOA,MAAK,MAAO,0BAA0B;AAC7C,eAAe;EACbC,IAAI,EAAE,gBAAgB;EACtBC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,UAAU,EAAE,IAAI,CAACC,MAAM,CAACC,KAAK,CAACF,UAAU;MACxCG,IAAI,EAAE,CAAC,CAAC;MACRC,eAAe,EAAE;QACfC,WAAW,EAAE,EAAE;QACfC,UAAU,EAAE,CAAC;QACbC,UAAU,EAAE,EAAE;QACdC,OAAO,EAAE;MACX;IACF,CAAC;EACH,CAAC;EACDC,OAAOA,CAAA,EAAG;IACR,IAAI,CAACN,IAAG,GAAI,IAAI,CAACO,kBAAkB,CAAC,MAAM,CAAC;EAC7C,CAAC;EACDC,UAAU,EAAE;IACVd;EACF,CAAC;EACDe,OAAO,EAAE;IACPC,cAAcA,CAAA,EAAG;MACf,IAAI,IAAI,CAACT,eAAe,CAACC,WAAU,IAAK,EAAE,EAAE;QAC1CS,KAAK,CAAC,YAAY,CAAC;QACnB;MACF;MACA,IAAI,IAAI,CAACV,eAAe,CAACG,UAAS,IAAK,EAAE,EAAE;QACzCO,KAAK,CAAC,YAAY,CAAC;QACnB;MACF;MACA,IAAI,IAAI,CAACV,eAAe,CAACI,OAAM,IAAK,EAAE,EAAE;QACtCM,KAAK,CAAC,YAAY,CAAC;QACnB;MACF;MAEAC,OAAO,CAACC,GAAG,CAAC,IAAI,CAACZ,eAAe,CAAC;MACjC,I