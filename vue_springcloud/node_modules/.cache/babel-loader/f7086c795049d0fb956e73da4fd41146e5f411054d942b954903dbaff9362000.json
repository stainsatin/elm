{"ast":null,"code":"import Footer from '../components/Footer.vue';\nexport default {\n  name: 'UpdateUserPassword',\n  data() {\n    return {\n      oldPass: '',\n      newPass: '',\n      user: {}\n    };\n  },\n  components: {\n    Footer\n  },\n  created() {\n    this.user = this.$getSessionStorage('user');\n  },\n  methods: {\n    toUpdateUserPassword() {\n      //根据userId更改password\n      if (this.oldPass == '') {\n        alert('请输入旧密码！');\n        return;\n      }\n      if (this.newPass == '') {\n        alert('新密码不能为空！');\n        return;\n      }\n      let url = 'UserController/updateUserPassword/${this.user.userId}/${this.oldPass}/${this.newPass}';\n      this.$axios.put(url).then(response => {\n        if (response.data.result > 0) {\n          this.user.password = this.newPass;\n          this.$setSessionStorage('user', this.user);\n          alert('修改密码成功！');\n        } else {\n          alert('修改密码失败！');\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["Footer","name","data","oldPass","newPass","user","components","created","$getSessionStorage","methods","toUpdateUserPassword","alert","url","$axios","put","then","response","result","password","$setSessionStorage","catch","error","console"],"sources":["C:\\Users\\admin\\Desktop\\elm_vue3\\src\\views\\UpdateUserPassword.vue"],"sourcesContent":["<template scoped>\r\n\t<!-- 总容器 -->\r\n\t<div class=\"wrapper\">\r\n\t\t<!-- header部分 -->\r\n\t\t<header>\r\n\t\t\t<p>更新密码</p>\r\n\t\t</header>\r\n\t\t<!-- 更新用户名部分 -->\r\n\t\t<div class=\"form-box\">\r\n\t\t\t<li>\r\n\t\t\t\t<div class=\"title\">\r\n\t\t\t\t\t旧密码:\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"content\">\r\n\t\t\t\t\t<input type=\"password\" v-model=\"oldPass\" placeholder=\"旧密码\">\r\n\t\t\t\t</div>\r\n\t\t\t</li>\r\n\t\t\t<li>\r\n\t\t\t\t<div class=\"title\">\r\n\t\t\t\t\t新密码:\r\n\t\t\t\t</div>\r\n\t\t\t\t<div class=\"content\">\r\n\t\t\t\t\t<input type=\"password\" v-model=\"newPass\" placeholder=\"新密码\">\r\n\t\t\t\t</div>\r\n\t\t\t</li>\r\n\t\t</div>\r\n\t\t<!--保存按钮-->\r\n\t\t<div class=\"save-button\" @click=\"toUpdateUserPassword()\">\r\n\t\t\t<button>保存</button>\r\n\t\t</div>\r\n\t\t<!-- 底部菜单部分 -->\r\n\t\t<Footer></Footer>\r\n\t</div>\r\n</template>\r\n\r\n<script>\r\n\timport Footer from '../components/Footer.vue';\r\n\t\r\n\texport default {\r\n\t\tname: 'UpdateUserPassword',\r\n\t\tdata() {\r\n\t\t\treturn {\r\n\t\t\t\toldPass: '',\r\n\t\t\t\tnewPass: '',\r\n\t\t\t\tuser: {}\r\n\t\t\t}\r\n\t\t},\r\n\t\tcomponents: {\r\n\t\t\tFooter\r\n\t\t},\r\n\t\tcreated() {\r\n\t\t\tthis.user = this.$getSessionStorage('user');\r\n\t\t},\r\n\t\tmethods: {\r\n\t\t\ttoUpdateUserPassword() {\r\n\t\t\t\t//根据userId更改password\r\n\t\t\t\tif (this.oldPass == '') {\r\n\t\t\t\t\talert('请输入旧密码！');\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n\t\t\t\tif (this.newPass == '') {\r\n\t\t\t\t\talert('新密码不能为空！');\r\n\t\t\t\t\treturn;\r\n\t\t\t\t}\r\n\t\t\t\tlet url='UserController/updateUserPassword/${this.user.userId}/${this.oldPass}/${this.newPass}';\r\n\t\t\t\tthis.$axios.put(url).then(response => {\r\n\t\t\t\t\tif (response.data.result > 0) {\r\n\t\t\t\t\t\tthis.user.password=this.newPass;\r\n\t\t\t\t\t\tthis.$setSessionStorage('user', this.user);\r\n\t\t\t\t\t\talert('修改密码成功！');\r\n\t\t\t\t\t} else {\r\n\t\t\t\t\t\talert('修改密码失败！');\r\n\t\t\t\t\t}\r\n\t\t\t\t}).catch(error => {\r\n\t\t\t\t\tconsole.error(error);\r\n\t\t\t\t});\r\n\t\t\t}\r\n\t\t\r\n\t\t}\r\n\t}\r\n</script>\r\n\r\n<style scoped>\r\n\t/****************** 总容器 ******************/\r\n\t.wrapper {\r\n\t\twidth: 100%;\r\n\t\theight: 100%;\r\n\t}\r\n\t\r\n\t/****************** header ******************/\r\n\t.wrapper header {\r\n\t\twidth: 100%;\r\n\t\theight: 12vw;\r\n\t\tbackground-color: deepskyblue;\r\n\t\tcolor: #fff;\r\n\t\tfont-size: 4.8vw;\r\n\t\tposition: fixed;\r\n\t\tleft: 0;\r\n\t\ttop: 0;\r\n\t\tz-index: 1000;\r\n\t\tdisplay: flex;\r\n\t\tjustify-content: center;\r\n\t\talign-items: center;