{"ast":null,"code":"import Footer from \"../components/Footer.vue\";\nexport default {\n  name: \"UpdateUserPassword\",\n  data() {\n    return {\n      oldPass: \"\",\n      newPass: \"\",\n      user: {}\n    };\n  },\n  components: {\n    Footer\n  },\n  created() {\n    this.user = this.$getSessionStorage(\"user\");\n  },\n  methods: {\n    toUpdateUserPassword() {\n      //根据userId更改password\n      if (this.oldPass == \"\") {\n        alert(\"请输入旧密码！\");\n        return;\n      }\n      if (this.oldPass != this.user.password) {\n        alert(\"请输入正确旧密码！\");\n        return;\n      }\n      if (this.newPass == \"\") {\n        alert(\"新密码不能为空！\");\n        return;\n      }\n      let url = `UserController/updateUserById/${this.user.userId}/${this.newPass}/${this.user.userName}/${this.user.userSex}/none`;\n      this.$axios.put(url).then(response => {\n        if (response.data.result > 0) {\n          this.user.password = this.newPass;\n          this.$setSessionStorage(\"user\", this.user);\n          alert(\"修改密码成功！\");\n        } else {\n          alert(\"修改密码失败！\");\n        }\n      }).catch(error => {\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["Footer","name","data","oldPass","newPass","user","components","created","$getSessionStorage","methods","toUpdateUserPassword","alert","password","url","userId","userName","userSex","$axios","put","then","response","result","$setSessionStorage","catch","error","console"],"sources":["C:\\WAILE\\Homework\\elm\\vue_springcloud\\src\\views\\UpdateUserPassword.vue"],"sourcesContent":["<template scoped>\r\n  <!-- 总容器 -->\r\n  <div class=\"wrapper\">\r\n    <!-- header部分 -->\r\n    <header>\r\n      <p>更新密码</p>\r\n    </header>\r\n    <!-- 更新用户名部分 -->\r\n    <div class=\"form-box\">\r\n      <li>\r\n        <div class=\"title\">旧密码:</div>\r\n        <div class=\"content\">\r\n          <input type=\"password\" v-model=\"oldPass\" placeholder=\"旧密码\" />\r\n        </div>\r\n      </li>\r\n      <li>\r\n        <div class=\"title\">新密码:</div>\r\n        <div class=\"content\">\r\n          <input type=\"password\" v-model=\"newPass\" placeholder=\"新密码\" />\r\n        </div>\r\n      </li>\r\n    </div>\r\n    <!--保存按钮-->\r\n    <div class=\"save-button\" @click=\"toUpdateUserPassword()\">\r\n      <button>保存</button>\r\n    </div>\r\n    <!-- 底部菜单部分 -->\r\n    <Footer></Footer>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport Footer from \"../components/Footer.vue\";\r\n\r\nexport default {\r\n  name: \"UpdateUserPassword\",\r\n  data() {\r\n    return {\r\n      oldPass: \"\",\r\n      newPass: \"\",\r\n      user: {},\r\n    };\r\n  },\r\n  components: {\r\n    Footer,\r\n  },\r\n  created() {\r\n    this.user = this.$getSessionStorage(\"user\");\r\n  },\r\n  methods: {\r\n    toUpdateUserPassword() {\r\n      //根据userId更改password\r\n      if (this.oldPass == \"\") {\r\n        alert(\"请输入旧密码！\");\r\n        return;\r\n      }\r\n      if (this.oldPass != this.user.password) {\r\n        alert(\"请输入正确旧密码！\");\r\n        return;\r\n      }\r\n      if (this.newPass == \"\") {\r\n        alert(\"新密码不能为空！\");\r\n        return;\r\n      }\r\n      let url = `UserController/updateUserById/${this.user.userId}/${this.newPass}/${this.user.userName}/${this.user.userSex}/none`;\r\n      this.$axios\r\n        .put(url)\r\n        .then((response) => {\r\n          if (response.data.result > 0) {\r\n            this.user.password = this.newPass;\r\n            this.$setSessionStorage(\"user\", this.user);\r\n            alert(\"修改密码成功！\");\r\n          } else {\r\n            alert(\"修改密码失败！\");\r\n          }\r\n        })\r\n        .catch((error) => {\r\n          console.error(error);\r\n        });\r\n    },\r\n  },\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/****************** 总容器 ******************/\r\n.wrapper {\r\n  width: 100%;\r\n  height: 100%;\r\n}\r\n\r\n/****************** header ******************/\r\n.wrapper header {\r\n  width: 100%;\r\n  height: 12vw;\r\n  background-color: deepskyblue;\r\n  color: #fff;\r\n  font-size: 4.8vw;\r\n  position: fixed;\r\n  left: 0;\r\n  top: 0;\r\n  z-index: 1000;\r\n  display: flex;\r\n  justify-content: center;\r\n  align-items: center;\r\n}\r\n\r\n/****************** 修改密码部分 ******************/\r\n.wrapper .form-box {\r\n  width: 100%;\r\n  padding-top: 12vw;\r\n}\r\n.wrapper .form-box li {\r\n  display: flex;\r\n  align-items: center;\r\n  box-sizing: border-box;\r\n  padding: 3vw 3vw 0;\r\n}\r\n.wrapper .form-box li .title {\r\n  flex: 0 0 18vw;\r\n  font-size: 3.5vw;\r\n  font-weight: 700;\r\n  color: darkslategray;\r\n}\r\n.wrapper .form-box li .content {\r\n  flex: 1;\r\n}\r\n.wrapper .form-box li .content input {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding: 4vw 2vw;\r\n  border-radius: 3vw;\r\n  background-color: white;\r\n\r\n  font-size: 3vw;\r\n  border: none;\r\n  outline: none;\r\n}\r\n\r\n/****************** 保存按钮部分 ******************/\r\n.wrapper .save-button {\r\n  width: 100%;\r\n  box-sizing: border-box;\r\n  padding: 4vw 10vw 0;\r\n}\r\n.wrapper .save-button button {\r\n  width: 100%;\r\n  height: 10vw;\r\n  background-color: mediumseagreen;\r\n  border: none;\r\n  outline: none;\r\n  border-radius: 4vw;\r\n\r\n  font-size: 3.5vw;\r\n  color: white;\r\n\r\n  cursor: pointer;\r\n}\r\n</style>\r\n"],"mappings":"AAgCA,OAAOA,MAAK,MAAO,0BAA0B;AAE7C,eAAe;EACbC,IAAI,EAAE,oBAAoB;EAC1BC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,OAAO,EAAE,EAAE;MACXC,OAAO,EAAE,EAAE;MACXC,IAAI,EAAE,CAAC;IACT,CAAC;EACH,CAAC;EACDC,UAAU,EAAE;IACVN;EACF,CAAC;EACDO,OAAOA,CAAA,EAAG;IACR,IAAI,CAACF,IAAG,GAAI,IAAI,CAACG,kBAAkB,CAAC,MAAM,CAAC;EAC7C,CAAC;EACDC,OAAO,EAAE;IACPC,oBAAoBA,CAAA,EAAG;MACrB;MACA,IAAI,IAAI,CAACP,OAAM,IAAK,EAAE,EAAE;QACtBQ,KAAK,CAAC,SAAS,CAAC;QAChB;MACF;MACA,IAAI,IAAI,CAACR,OAAM,IAAK,IAAI,CAACE,IAAI,CAACO,QAAQ,EAAE;QACtCD,KAAK,CAAC,WAAW,CAAC;QAClB;MACF;MACA,IAAI,IAAI,CAACP,OAAM,IAAK,EAAE,EAAE;QACtBO,KAAK,CAAC,UAAU,CAAC;QACjB;MACF;MACA,IAAIE,GAAE,G